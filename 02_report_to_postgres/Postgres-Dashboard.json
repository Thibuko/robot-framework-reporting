          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH FailuresCount AS (\r\n  SELECT\r\n    test,\r\n    testsuite,\r\n    COUNT(*) FILTER (WHERE status = 'FAIL' AND endtime > endtime - INTERVAL '14 days') AS failures_in_last_14_days,\r\n    COUNT(*) FILTER (WHERE status = 'PASS' AND endtime > endtime - INTERVAL '14 days') AS passed_in_last_14_days\r\n  FROM\r\n    test_results\r\n\r\n  GROUP BY\r\n    test, testsuite\r\n)\r\nSELECT\r\n  tr.test,\r\n  tr.testsuite,\r\n  tr.status,\r\n  COALESCE(fc.failures_in_last_14_days, 0) AS \"Failures in last 14 days\",\r\n  COALESCE(fc.passed_in_last_14_days, 0) AS \"Passed in last 14 days\"\r\nFROM\r\n  test_results tr\r\nLEFT JOIN FailuresCount fc ON tr.test = fc.test AND tr.testsuite = fc.testsuite\r\nWHERE tr.status = 'FAIL' AND tr.metadata->>'version'='$version' AND tr.metadata->>'environment'='$environment' AND tr.metadata->>'project'='$project' AND tr.metadata->>'test_run'='$run'\r\nGROUP BY\r\n  tr.test, tr.testsuite, tr.status, \"Failures in last 14 days\", \"Passed in last 14 days\"\r\nORDER BY\r\n  tr.test, tr.testsuite;\r\n",
